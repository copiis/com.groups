                "prev": "capabilities"
            }
        },
        {
            "id": "icons",
            "$template": "icons",
            "navigation": {
                "next": "group",
                "prev": "devices"
            }
        },
        {
            "id": "group",
            "$template": "group",
            "navigation": {
                "prev": "icons"
            }
        }
    ],
    "settings" : [
        {
            "$extends":  "about"
        },
        {
            "$extends":  "set"
        },
        {
            "$extends":  "other"
        }
    ]
}
